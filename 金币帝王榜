            url = f'https://{port}/hex/member/bet_statistics/overview'

            headers = {
                'accept': 'application/json, text/plain, */*',
                'accept-language': 'zh-CN,zh;q=0.9',
                'content-type': 'application/json',
                'permname': 'GeneralMember',
