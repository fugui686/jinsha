    def initUI(self):
        self.setWindowTitle('ÈáëÂ∏Å/Â∏ùÁéãÊ¶ú__ÈáëÊ≤ô')
        self.resize(600, 500)

        # ‰∏ªÂ∏ÉÂ±Ä
        main_layout = QVBoxLayout()

        # ÂàõÂª∫‰∏Ä‰∏™ÁªÑÂÆπÂô®ÔºàGroupBoxÔºâ
        group_box = QGroupBox("Êìç‰ΩúÈù¢Êùø")
        group_layout = QVBoxLayout()

        # ÂàõÂª∫ÂêéÂè∞ÂüüÂêçËæìÂÖ•Ê°Ü
        self.srk_port = QLineEdit()
        self.srk_port.setPlaceholderText('ËØ∑ËæìÂÖ•ÂêéÂè∞ÂüüÂêçÔºå‰æãÂ¶ÇÔºöcr168.mne-xoek35.com')
        group_layout.addWidget(self.srk_port)

        # ÂàõÂª∫ Cookie ËæìÂÖ•Ê°Ü
        self.srk_token = QLineEdit()
        self.srk_token.setPlaceholderText('ËØ∑ËæìÂÖ•Cookie')
        group_layout.addWidget(self.srk_token)

        # ‰øùÂ≠òÈÖçÁΩÆÊåâÈíÆ
        self.save_button = QPushButton("üíæ ‰øùÂ≠òÈÖçÁΩÆ")
        self.save_button.clicked.connect(self.manual_save)
        group_layout.addWidget(self.save_button)

        # Ëé∑ÂèñÈáëÂ∏ÅÊï∞ÊçÆÊåâÈíÆ
        self.submit_button = QPushButton('üöÄ Ëé∑ÂèñÈáëÂ∏ÅÊï∞ÊçÆ')
        self.submit_button.clicked.connect(self.print_accoun)
        group_layout.addWidget(self.submit_button)

        # Ëé∑ÂèñÂ∏ùÁéãÊ¶úÊï∞ÊçÆÊåâÈíÆ
        self.gold_button = QPushButton('üöÄ Ëé∑ÂèñÂ∏ùÁéãÊ¶úÊï∞ÊçÆ')
        self.gold_button.clicked.connect(self.print_accounn)
        group_layout.addWidget(self.gold_button)

        # ÊääÁªÑÂ∏ÉÂ±ÄËÆæÁΩÆÂà∞GroupBox‰∏≠
        group_box.setLayout(group_layout)

        # ÊääGroupBoxÂä†Âà∞‰∏ªÂ∏ÉÂ±Ä‰∏≠
        main_layout.addWidget(group_box)

        # ËÆæÁΩÆ‰∏ªÁ™óÂè£Â∏ÉÂ±Ä
        self.setLayout(main_layout)

        self.load_config()

    def save_config(self, port, token):  # ‰øùÂ≠òÈÖçÁΩÆÂà∞Êñá‰ª∂
        config = ConfigParser()
        config['USER'] = {'Port': port, 'Token': token}
        with open('config.ini', 'w') as configfile:
            config.write(configfile)

    def load_config(self):  # ‰ªéÊñá‰ª∂Âä†ËΩΩÈÖçÁΩÆ
        config = ConfigParser()
        config.read('config.ini')
        if 'USER' in config:
            self.srk_port.setText(config['USER']['Port'])
            self.srk_token.setText(config['USER']['Token'])

    def print_accoun(self):  # ÈªòËÆ§Ëé∑ÂèñÊò®Êó•ÈáëÂ∏ÅÊï∞ÊçÆ
        try:
            port = self.srk_port.text().strip()
            token = self.srk_token.text()

            if not port or not token:
                QMessageBox.warning(self, "ËæìÂÖ•ÈîôËØØ", "ËØ∑Á°Æ‰øùÊâÄÊúâËæìÂÖ•Ê°ÜÈÉΩÂ∑≤Â°´ÂÜôÔºÅ")
                return

            # Ëé∑ÂèñÊú¨Âú∞Êó∂Èó¥
            local_time = datetime.now()

            # ÂÆö‰πâÁæé‰∏úÊó∂Âå∫
            eastern_tz = pytz.timezone('US/Eastern')

            # Â∞ÜÊú¨Âú∞Êó∂Èó¥ËΩ¨Êç¢‰∏∫Áæé‰∏úÊó∂Èó¥
            eastern_time = local_time.astimezone(eastern_tz)

            # Ëé∑ÂèñÊò®Â§©ÁöÑÊó∂Èó¥
            yesterday = eastern_time - timedelta(days=1)

            # Ê†ºÂºèÂåñÊò®Â§©ÁöÑÊó∂Èó¥
            Êò®Â§© = yesterday.strftime('%Y-%m-%d')

            url = f'https://{port}/hex/member/bet_statistics/overview'

            headers = {
                'accept': 'application/json, text/plain, */*',
                'accept-language': 'zh-CN,zh;q=0.9',
                'content-type': 'application/json',
                'permname': 'GeneralMember',
                'cookie': token,
            }

            json_data = {
                'date_type': 1,
                'start_date': Êò®Â§©,
                'end_date': Êò®Â§©,
                'search_mode': 'user',
                'usernames': [],
                'sort': 'commissionable',
                'order': 'desc',
                'page': 1,
                'limit': 1000,
                'filter_type': 'wagers',
                'lobbys': [
                    5, 32, 37, 39, 40, 41, 44, 46,
                    50, 52, 53, 58, 59, 71, 79, 82, 114, 116,
                    117, 118, 120, 124, 132, 133, 138, 144, 169, 174,
                ],
            }

            response = requests.post(url=url, headers=headers, json=json_data, verify=False)
            if response.status_code == 200:
                data = response.json().get('data').get('list')

                wb = Workbook()
                ws = wb.active
                ws.append(['‰ºöÂëòË¥¶Âè∑', 'ÊúâÊïàÊäïÊ≥®'])
                for aa in data:
                    ‰ºöÂëòË¥¶Âè∑ = aa.get('user_account')
                    ÊúâÊïàÊäïÊ≥® = aa.get('bet').get('commissionable')
                    Â±ÇÁ∫ß = aa.get('level_id')
                    if Â±ÇÁ∫ß == 2776:  # 2776ÊòØÊ¥óÊ∞¥Â±ÇÁ∫ßÔºå‰∏çÈúÄË¶ÅËÆ°ÁÆó
                        continue
                    if Â±ÇÁ∫ß == 16066:  # 16066ÊòØ‰∏çÁªôËøîÊ∞¥Â±ÇÁ∫ßÔºå‰∏çÈúÄË¶ÅËÆ°ÁÆó
                        continue
                    ws.append([‰ºöÂëòË¥¶Âè∑, ÊúâÊïàÊäïÊ≥®])

                # ‰øùÂ≠òÊñá‰ª∂Âà∞Ê°åÈù¢
                desktop_path = os.path.join(os.path.expanduser('~'), 'Desktop')
                file_path = os.path.join(desktop_path, f'ÈáëÂ∏Å{Êò®Â§©}.xls')
                wb.save(file_path)
                os.startfile(file_path)

        except Exception as e:
            QMessageBox.critical(self, "ÈîôËØØ", str(e))

    def print_accounn(self):  # ÈªòËÆ§Ëé∑ÂèñÊò®Êó•Â∏ùÁéãÊ¶úÊï∞ÊçÆ
