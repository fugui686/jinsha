                data = response.json().get('data').get('list')

                wb = Workbook()
                ws = wb.active
                ws.append(['Áî®Êà∑Âêç', 'Âë®ÊúüÂàÜÁ±ª', 'ÊúâÊïàÊäïÊ≥®'])
                for aa in data:
                    ‰ºöÂëòË¥¶Âè∑ = aa.get('user_account')
                    ÊúâÊïàÊäïÊ≥® = aa.get('bet').get('commissionable')
                    Â±ÇÁ∫ß = aa.get('level_id')
                    if Â±ÇÁ∫ß == 2776:  # 2776ÊòØÊ¥óÊ∞¥Â±ÇÁ∫ßÔºå‰∏çÈúÄË¶ÅËÆ°ÁÆó
                        continue
                    if Â±ÇÁ∫ß == 16066:  # 16066ÊòØ‰∏çÁªôËøîÊ∞¥Â±ÇÁ∫ßÔºå‰∏çÈúÄË¶ÅËÆ°ÁÆó
                        continue
                    # Ê†ºÂºèÂåñÊò®Â§©ÁöÑÊó∂Èó¥
                    zt = yesterday.strftime('%mÊúà%dÊó•')
                    rq = f'{zt}-{zt}'
                    ws.append([‰ºöÂëòË¥¶Âè∑, rq, ÊúâÊïàÊäïÊ≥®])

                # ‰øùÂ≠òÊñá‰ª∂Âà∞Ê°åÈù¢
                desktop_path = os.path.join(os.path.expanduser('~'), 'Desktop')
                file_path = os.path.join(desktop_path, f'Â∏ùÁéãÊ¶ú{Êò®Â§©}.xls')
                wb.save(file_path)
                os.startfile(file_path)

        except Exception as e:
            QMessageBox.critical(self, "ÈîôËØØ", str(e))

    def manual_save(self):  # ÊâãÂä®‰øùÂ≠òÈÖçÁΩÆ
        port = self.srk_port.text()
        token = self.srk_token.text()

        if not (port and token):
            QMessageBox.warning(self, "ËæìÂÖ•ÈîôËØØ", "ËØ∑Â°´ÂÜôÊâÄÊúâÂ≠óÊÆµÔºÅ")
            return

        self.save_config(port, token)
        QMessageBox.information(self, "ÊèêÁ§∫", "ÈÖçÁΩÆÂ∑≤‰øùÂ≠òÔºÅ")


if __name__ == '__main__':
    app = QApplication(sys.argv)
    login_window = LoginWindow()
    login_window.show()
    sys.exit(app.exec_())
